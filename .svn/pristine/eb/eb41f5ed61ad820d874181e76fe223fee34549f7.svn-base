<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags always come first -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="css/bootstrap.min.css">

    <!-- Custom CSS -->
    <link rel="stylesheet" href="css/profile.css">
    
    <!-- jQuery first, then Tether, then Bootstrap JS. -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular.min.js"></script>
    <script src="js/lib/bootstrap.min.js"></script>

    <!-- JS Angular -->
    <script src="js/lib/angular.min.js"></script>
    <script src="js/lib/angular-route.min.js"></script>
    <script src="js/lib/angular-animate.min.js"></script>
    
    <!-- Skills Controller -->
    <script src="js/controllers/SkillsController.js"></script>
    
</head>

<body>
    <!-- Nav Bar -->
    <nav class="navbar navbar-light bg-faded navbar-fixed-top">
        <button class="navbar-toggler hidden-lg-up" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation"></button>
        <div class="collapse navbar-toggleable-md" id="navbarResponsive">
            <a class="navbar-brand" href="index.html">
                <img src="assets/brand/scc.png" height="35" alt=""> Senior Companion Care</a>
            <ul class="nav navbar-nav float-xs-right">
                <li class="nav-item">
                    <a class="nav-link" href="index.html">Home <span class="sr-only">(current)</span></a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="scc.html">SCC</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="search.html">Search</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="contact.html">Contact</a>
                </li>
                <li class="nav-item">
                    <a class="btn btn-danger" href="login.html">Log Out</a>
                </li>
            </ul>
        </div>
    </nav>

    <!-- My Skills -->
    <section id="main-content">
        <div class="section-content">
            <div class="container">
                <div class="col-sm-12">
                    <div class="about-text">
                        <h1 class="display-4">My Skills &amp Schedule</h1>
                        <hr>
                    </div>
                    <br>
                     <div class="form-content" ng-app="skillsApp" ng-controller="SkillsController">
                        <div class="form-group row">
                            <div class="skills-selection">
                                <div class="skills">
                                    <h3>Type Of Care:</h3>
                                    <br>
                                    <div class="checkbox">
                                        <div class="col-sm-3">
                                            <label><input type="checkbox" value=""> Parkinson's Care </label>
                                        </div>
                                    </div>
                                    <div class="checkbox">
                                        <div class="col-sm-3">
                                            <label><input type="checkbox" value=""> Alzheimer's Care </label>
                                        </div>
                                    </div>
                                    <div class="checkbox">
                                        <div class="col-sm-3">
                                            <label><input type="checkbox" value=""> Stroke Care </label>
                                        </div>
                                    </div>
                                    <div class="checkbox">
                                        <div class="col-sm-3">
                                            <label><input type="checkbox" value=""> Cancer Care </label>
                                        </div>
                                    </div>
                                    <div class="checkbox">
                                        <div class="col-sm-3">
                                            <label><input type="checkbox" value=""> Hospital Care </label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <br>
                        </div>
                        <hr>
                        <div class="form-group row">
                            <div class="schedule-selection">
                                <div class="schedule">
                                    <h3>Availability Schedule:</h3>
                                    <br>
                                    <div class="checkbox">
                                        <div class="col-sm-3">
                                            <label><input type="checkbox" value=""> Monday </label>
                                        </div>
                                    </div>
                                    <div class="checkbox">
                                        <div class="col-sm-3">
                                            <label><input type="checkbox" value=""> Tuesday </label>
                                        </div>
                                    </div>
                                    <div class="checkbox">
                                        <div class="col-sm-3">
                                            <label><input type="checkbox" value=""> Wednesday </label>
                                        </div>
                                    </div>
                                    <div class="checkbox">
                                        <div class="col-sm-3">
                                            <label><input type="checkbox" value=""> Thursday </label>
                                        </div>
                                    </div>
                                    <div class="checkbox">
                                        <div class="col-sm-3">
                                            <label><input type="checkbox" value=""> Friday </label>
                                        </div>
                                    </div>
                                    <div class="checkbox">
                                        <div class="col-sm-3">
                                            <label><input type="checkbox" value=""> Saturday </label>
                                        </div>
                                    </div>
                                    <div class="checkbox">
                                        <div class="col-sm-3">
                                            <label><input type="checkbox" value=""> Sunday </label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <hr>
                        <div class="form-group row">
                            <div class="resume-section">
                                <div class="col-sm-11">
                                    <div class="resume">
                                        <h3><label for="resume">Work Experience/ Resume:</label></h3>
                                        <br>
                                        <textarea class="form-control" rows="3" id="resume"></textarea>
                                        <br>
                                        <button type="submit" class="btn btn-lg btn-success">Submit</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</body>

</html>
